{
    "main": "bin/raml-code-generator",
    "output": "dist/raml-code-generator.phar",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php"
    ],
    "finder": [
        {
            "name": ["*.php", "*.xml", "*.yml", "*.xsd", "*.db", "*.twig"],
            "exclude": ["test", "Tests", "tests"],
            "in": ["src", "vendor", "app"]
        }
    ],
    "blacklist": [
        "vendor-bin"
    ],
    "git-version": "package_version"
}
